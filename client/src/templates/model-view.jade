.y-model-view
  .clearfix
    .y-tag Model
    .y-tagline(data-bind='text:key')
  h1.y-title(data-bind='text:title')
  .y-section(data-bind='geyser:summary')
  h2.y-subtitle(data-bind='collapse:false') Parameters
  .y-parameters(data-bind='geyser:parameters')
  h2.y-subtitle(data-bind='collapse:false') Input Columns 
    span(data-bind='text:inputColumnsCount')
  .y-input-columns(data-bind='geyser:inputColumns')
  h2.y-subtitle(data-bind='collapse:false') Compatible Datasets 
    span(data-bind='text:compatibleFramesCount')
  div
    .y-frames(data-bind='geyser:compatibleFrames')
    .y-controls
      button.btn(data-bind='click:loadCompatibleFrames') View These Datasets&hellip;
  h2.y-subtitle(data-bind='collapse:true') JSON
  pre(data-bind='json:data')

